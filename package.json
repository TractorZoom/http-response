{
    "name": "@tractorzoom/http-response",
    "version": "2.1.18",
    "description": "Library to build http responses for use in serverless projects",
    "main": "lib/index.js",
    "author": "@TractorZoom",
    "license": "MIT",
    "engines": {
        "node": "12.x"
    },
    "scripts": {
        "build": "./node_modules/.bin/babel src --out-dir lib --copy-files",
        "coverage": "jest --coverage",
        "test": "jest",
        "commit": "git-cz",
        "prettier:all": "prettier --write './**/*.*'",
        "pretty-quick": "pretty-quick"
    },
    "devDependencies": {
        "@babel/cli": "7.10.5",
        "@babel/core": "7.11.4",
        "@babel/preset-env": "7.11.0",
        "@commitlint/cli": "8.3.5",
        "@commitlint/config-conventional": "8.3.4",
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/git": "9.0.0",
        "@semantic-release/release-notes-generator": "9.0.1",
        "chance": "1.1.6",
        "commitizen": "4.1.5",
        "husky": "4.2.5",
        "jest": "26.4.1",
        "prettier": "2.0.5",
        "pretty-quick": "2.0.2",
        "semantic-release": "17.1.1"
    },
    "dependencies": {
        "http-status-codes": "1.4.0"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TractorZoom/http-response.git"
    },
    "homepage": "https://github.com/TractorZoom/http-response",
    "bugs": {
        "url": "https://github.com/TractorZoom/http-response/issues"
    },
    "keywords": [
        "aws",
        "aws-lambda",
        "api-gateway",
        "http",
        "response"
    ]
}
